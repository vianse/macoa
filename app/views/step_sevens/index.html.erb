<p id="notice"><%= notice %></p>

<h1>Step Sevens</h1>

<table>
  <thead>
    <tr>
      <th>Fecharevisionprevia</th>
      <th>Pedido</th>
      <th>User</th>
      <th>Completado</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @step_sevens.each do |step_seven| %>
      <tr>
        <td><%= step_seven.fechaRevisionPrevia %></td>
        <td><%= step_seven.pedido %></td>
        <td><%= step_seven.user_id %></td>
        <td><%= step_seven.completado %></td>
        <td><%= link_to 'Show', step_seven %></td>
        <td><%= link_to 'Edit', edit_step_seven_path(step_seven) %></td>
        <td><%= link_to 'Destroy', step_seven, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Step Seven', new_step_seven_path %>
