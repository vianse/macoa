<p id="notice"><%= notice %></p>

<h1>Step Fours</h1>

<table>
  <thead>
    <tr>
      <th>Accesorio</th>
      <th>Inicio</th>
      <th>Fin</th>
      <th>Pedido</th>
      <th>User</th>
      <th>Completado</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @step_fours.each do |step_four| %>
      <tr>
        <td><%= step_four.accesorio %></td>
        <td><%= step_four.inicio %></td>
        <td><%= step_four.fin %></td>
        <td><%= step_four.pedido %></td>
        <td><%= step_four.user_id %></td>
        <td><%= step_four.completado %></td>
        <td><%= link_to 'Show', step_four %></td>
        <td><%= link_to 'Edit', edit_step_four_path(step_four) %></td>
        <td><%= link_to 'Destroy', step_four, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Step Four', new_step_four_path %>
