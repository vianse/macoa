<p id="notice"><%= notice %></p>

<h1>Step Nines</h1>

<table>
  <thead>
    <tr>
      <th>Sala</th>
      <th>Auto</th>
      <th>Factura</th>
      <th>Pedido</th>
      <th>User</th>
      <th>Completado</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @step_nines.each do |step_nine| %>
      <tr>
        <td><%= step_nine.sala %></td>
        <td><%= step_nine.auto %></td>
        <td><%= step_nine.factura %></td>
        <td><%= step_nine.pedido %></td>
        <td><%= step_nine.user_id %></td>
        <td><%= step_nine.completado %></td>
        <td><%= link_to 'Show', step_nine %></td>
        <td><%= link_to 'Edit', edit_step_nine_path(step_nine) %></td>
        <td><%= link_to 'Destroy', step_nine, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Step Nine', new_step_nine_path %>
