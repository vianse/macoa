<p id="notice"><%= notice %></p>

<h1>Tipo Venta</h1>

<table>
  <thead>
    <tr>
      <th>Tipoventa</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tipo_venta.each do |tipo_ventum| %>
      <tr>
        <td><%= tipo_ventum.tipoVenta %></td>
        <td><%= link_to 'Show', tipo_ventum %></td>
        <td><%= link_to 'Edit', edit_tipo_ventum_path(tipo_ventum) %></td>
        <td><%= link_to 'Destroy', tipo_ventum, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

